{
    "extends": "standard",
    "plugins": [
        "standard",
        "promise"
    ],
    "rules": {
      "indent": [2, 2],
      "semi": [2, "always"],
      "comma-dangle": 0,
      "space-before-blocks": [0, "always"],
      "space-before-function-paren": [0, "always"],
      "keyword-spacing": [2, {
        "before": false, "after": false,
        "overrides": {
          "return": { "after": true }
        }
      }]
    }
}
